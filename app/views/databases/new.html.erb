<% provide( :title, t( "databases.new.title")) %>
<%= render partial: "databases/warning" %>
    <%= form_for( @database, :url => database_path,
                             html: { class: "form-inline"}) do |f| %>
      <%= f.label :all_data, t( "database.create.label") %>
      <%= f.file_field :all_data %>
      <%= f.submit t( "general.ok"),
                   data: { confirm: t( "database.create.confirm")} %>
    <% end %>

