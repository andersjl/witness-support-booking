<% # encoding: UTF-8 %>
<% provide( :title, @court.name) %>
<h1>Ändra domstol <%= @court.name %></h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for @court do |f| %> 
      <%= render :partial => "shared/error_messages",
                 :locals => { :model => @court} %>
      <%= f.label :name, "Namn" %>
      <%= f.text_field :name %>
      <%= f.label :link, "Länk till hemsida" %>
      <%= f.text_field :link %>
      <%= f.submit "Ändra" %>
    <% end %>
  </div>
</div>

