          <%= form_for note, html: { class: "form-inline"} do |f| %>
<%  if note.new_record? %>
            <%= f.hidden_field :court_id %>
            <%= f.hidden_field :date %>
<%  end %>
            <%= f.text_area :text, onchange: "this.form.submit();" %>
<%  if Rails.env.test? %>
            <%= f.submit "test dummy" %>
<%  end %>
          <% end %>
