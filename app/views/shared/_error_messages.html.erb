<% if @user.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-error">
      Formuläret innehåller <%= @user.errors.count %> fel
    </div>
    <ul>
      <% @user.errors.full_messages.each do |msg| %>
        <li>* <%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>

