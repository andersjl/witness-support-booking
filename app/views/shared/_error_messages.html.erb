<% if model.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-error">
      Formuläret innehåller <%= model.errors.count %> fel
    </div>
    <ul>
      <% model.errors.full_messages.each do |msg| %>
        <li>* <%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>

